		"chartjs-chart-venn": "^4.1.1",
		"emoji-picker-element": "^1.15.0",
		"highcharts": "^10.3.3",
		"highcharts-react-official": "^3.1.0",
		"highlight.js": "11.5.1",
		"linkifyjs": "^4.1.0",
		"minidenticons": "^2.0.0",
		"nanoid": "^2.1.9",
		"node-sass": "^8.0.0",
